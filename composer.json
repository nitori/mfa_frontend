{
  "name": "causal/mfa-frontend",
  "type": "typo3-cms-extension",
  "description": "Enable MFA (Multi-Factor Authentication) for Frontend accounts.",
  "keywords": [
    "TYPO3 CMS",
    "MFA",
    "multi-factor",
    "2FA",
    "authentication",
    "otp",
    "one-time",
    "secure",
    "security",
    "protection"
  ],
  "homepage": "https://extensions.typo3.org/extension/mfa_frontend",
  "support": {
    "docs": "https://docs.typo3.org/p/causal/mfa-frontend/main/en-us/",
    "issues": "https://github.com/xperseguers/mfa_frontend/issues",
    "source": "https://github.com/xperseguers/mfa_frontend"
  },
  "authors": [
    {
      "name": "Xavier Perseguers",
      "email": "xavier@causal.ch",
      "homepage": "https://www.causal.ch",
      "role": "Developer"
    }
  ],
  "license": "GPL-3.0-or-later",
  "require": {
    "php": ">=8.0.0 <=8.3.99",
    "typo3/cms-core": "^11 || ^12",
    "chillerlan/php-qrcode": "^4.3 || 5.0-beta"
  },
  "autoload": {
    "psr-4": {
      "Causal\\MfaFrontend\\": "Classes/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.2.x-dev"
    },
    "typo3/cms": {
      "extension-key": "mfa_frontend"
    }
  }
}
