            <trans-unit id="qrImage">
                <source>Google Authenticator QR-Code</source>
            </trans-unit>
            <trans-unit id="otp">
                <source>One-time password</source>
            </trans-unit>

            <trans-unit id="backend.disableAuthenticator">
                <source>To disable the Two-Factor Authentication, toggle the switch above off and enter a valid one-time password.</source>
            </trans-unit>

            <trans-unit id="totp.step1">
                <source>Step 1</source>
            </trans-unit>
